{
  "name": "anima-insights",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "NODE_OPTIONS='--loader=ts-node/esm' ts-node src/server.ts",
    "dev:server:auth": "NODE_OPTIONS='--loader=ts-node/esm' ts-node src/server/routes.auth.ts",
    "dev:all": "concurrently \"npm run dev\" \"tsc -p numinos-service/tsconfig.json -w\" \"nodemon --watch numinos-service/dist/index.js --delay 1 --exec \"node numinos-service/dist/index.js\"\"",
    "build": "vite build",
    "start": "node dist/server.js",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:json": "eslint . -f json -o reports/eslint-warnings.json || true",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check:client": "tsc -p tsconfig.client.json --noEmit",
    "type-check:server": "tsc -p tsconfig.server.json --noEmit",
    "type-check": "npm run type-check:client && npm run type-check:server",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "db:setup": "node scripts/setup-db.js",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "test:auth": "vitest run src/server/auth/*.test.ts",
    "validate": "npm run lint && npm run type-check && npm run test --if-present",
    "clean": "rimraf dist",
    "security:audit": "npm audit",
    "update:check": "ncu",
    "update:apply": "ncu -u && npm install",
    "prepare": "husky install",
    "dev:numinos": "node ./scripts/dev-journal-server.js",
    "dev:server:fast": "cross-env PORT=3001 tsx src/server.ts",
    "openapi:lint": "spectral lint docs/openapi.yaml"
    ,"openapi:gen": "tsx scripts/generate-openapi.ts"
    ,"secrets:scan": "mkdir -p reports && bash scripts/run-gitleaks.sh detect --no-git --redact -f json -r reports/gitleaks-workspace.json -c .gitleaks.toml"
    ,"secrets:scan:staged": "mkdir -p reports && bash scripts/run-gitleaks.sh protect --staged --redact -f json -r reports/gitleaks-staged.json -c .gitleaks.toml"
    ,"secrets:scan:full": "mkdir -p reports && bash scripts/run-gitleaks.sh detect --redact -f json -r reports/gitleaks-full.json -c .gitleaks.toml"
  },
  "dependencies": {
    "@fontsource/cinzel": "^5.0.8",
    "@fontsource/cormorant-garamond": "^5.0.8",
    "@fontsource/unifrakturmaguntia": "^5.0.8",
    "@simplewebauthn/server": "^13.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.1.1",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.29.0",
    "express": "^4.21.2",
    "framer-motion": "^10.16.4",
    "helmet": "^8.1.0",
    "jose": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "libsodium-wrappers": "^0.7.15",
    "lines-and-columns": "^2.0.4",
    "lucide-react": "^0.292.0",
    "nanoid": "^5.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.0",
    "otplib": "^12.0.1",
    "pino": "^9.0.0",
    "pino-http": "^11.0.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-textarea-autosize": "^8.5.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.4",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@stoplight/spectral-cli": "^6.15.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^5.0.3",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^20.19.18",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "nodemon": "^3.0.1",
    "npm-check-updates": "^16.14.11",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.1",
    "typescript": "^5.9.2",
    "vite": "^4.5.14",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
