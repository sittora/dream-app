{
  "name": "anima-insights",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
  "dev:all": "concurrently \"npm run dev\" \"tsc -p numinos-service/tsconfig.json -w\" \"nodemon --watch numinos-service/dist/index.js --delay 1 --exec \"node numinos-service/dist/index.js\"\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
  "lint": "eslint \"src/**/*.{ts,tsx}\" \"numinos-service/src/**/*.{ts,tsx}\" \"scripts/**/*.{ts,js}\" || true",
  "lint:fix": "eslint \"src/**/*.{ts,tsx}\" \"numinos-service/src/**/*.{ts,tsx}\" --fix || true",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "db:setup": "node scripts/setup-db.js",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "validate": "npm run lint && npm run type-check",
    "clean": "rimraf dist",
    "security:audit": "npm audit",
    "update:check": "ncu",
    "update:apply": "ncu -u && npm install",
    "prepare": "husky install",
    "dev:numinos": "node ./scripts/dev-journal-server.js"
  },
  "dependencies": {
    "@fontsource/cinzel": "^5.0.8",
    "@fontsource/cormorant-garamond": "^5.0.8",
    "@fontsource/unifrakturmaguntia": "^5.0.8",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.1.1",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.29.0",
    "framer-motion": "^10.16.4",
    "jose": "^5.1.0",
    "lines-and-columns": "^2.0.4",
    "lucide-react": "^0.292.0",
    "nanoid": "^5.0.3",
    "openai": "^4.20.0",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "react-textarea-autosize": "^8.5.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^5.0.3",
    "@types/node": "^20.9.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.4",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "npm-check-updates": "^16.14.11",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.6.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },

  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
